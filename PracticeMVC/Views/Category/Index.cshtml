
@{
    ViewData["Title"] = "Category Page";
}
@model IEnumerable<CategoryModel>

<div>
    <h1>
        CATEGORIES
    </h1>
    <table class="table table-bordered table-striped" style="width:100%">
        <thead>
            <tr>
                <th>ID</th>
                <th>NAME</th>
                <th>DESCRIPTION</th>
                <th>CategoryId</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var obj in Model)
            {
                <tr>
                    <td>@obj.Id</td>
                    <td>@obj.Name</td>
                    <td>@obj.Description</td>
                    <td>@obj.CategoryId</td>
                </tr>

            }
           
        </tbody>
    </table>
</div>

